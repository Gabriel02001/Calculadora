<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Historico</title>
     <!-- VIEWPORT -->
  
  <!-- GOOGLE STYLESHEET || FONTS -->
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Anton&family=DM+Sans:wght@400;500;700&family=Mitr:wght@600&family=Pacifico&family=Poppins:wght@700&display=swap" rel="stylesheet">

		<!-- Latest compiled and minified CSS -->
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">

    <!-- MY STYLESHEETS || CSS -->
<link rel="stylesheet" href="/styles/global.css">
<link rel="stylesheet" href="/styles/historico.css">
</head>
<body>
    
    
  <main class="content">
      <ul class="list_operations">
          <h1>Hist√≥rico</h1>

         <% const listObj = JSON.parse(list) %> 
         <% listObj.forEach(item => { %>
          <li class="operation" > 
            <div class="card">
             <div class="card-body">
              <h2> <strong> <%=  item.resultado %></strong></h2> 
              <p>  <%= item.conta %> </p>
              <br>
               <div id="time"><h4> time: <%=  item.create_at %></h4></div>
             </div>
             <span id="operation_id"> ID: <%=  item.id %></span>
            </div>
          </li>
        <% }); %>

    </ul>
    <a  href="/">Calculadora</a>
 </main>

    
    
</body>
</html>